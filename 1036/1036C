#include <stdio.h>
#include <math.h>
 
int main() {

    double A, B, C, bas, R1, R2;
    
    scanf("%lf %lf %lf", &A, &B, &C);
    
    bas = (B * B) - (4 * A * C);
    
    if (bas < 0 || A == 0) {
        printf("Impossivel calcular\n");
    } else {
        bas = sqrt(bas);
        R1 = (-B + bas) / (2 * A);
        R2 = (-B - bas) / (2 * A);
        printf("R1 = %.5f\n", R1);
        printf("R2 = %.5f\n", R2);
    }
    
    
    return 0;
}