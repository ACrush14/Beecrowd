#include <stdio.h>
#include <iostream>
#include <string.h>
 
int main() {
 
    char a[20], b[20], c[20];
    
    std::cin >> a >> b >> c;
    
    if (strcmp(a, "vertebrado") == 0) {
        if (strcmp(b, "ave") == 0) {
            if (strcmp(c, "carnivoro") == 0) {
                std::cout << "aguia\n";
            }
            else {
                std::cout << "pomba\n";
            }
        }
        else if(strcmp(b, "mamifero") == 0) {
            if (strcmp(c, "onivoro") == 0) {
                std::cout << "homem\n";
            }
            else {
                std::cout << "vaca\n";
            }
        }
    } else if (strcmp(a, "invertebrado") == 0) {
        if (strcmp(b, "inseto") == 0) {
            if (strcmp( c, "hematofago") == 0) {
                std::cout << "pulga\n";
            } else {
                std::cout << "lagarta\n";
            }
        }
        else if (strcmp(b, "anelideo") == 0) {
            if (strcmp(c, "hematofago") == 0) {
                std::cout << "sanguessuga\n";
            } else {
                std::cout << "minhoca\n";
            }
        }
    }

 
    return 0;
}