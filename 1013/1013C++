#include <stdio.h>
#include <iostream>
 
int main() {

    int a, b, c, maiorAB, maiorABC;
    
    std::cin >> a >> b >> c;
    
    maiorAB = (a + b +abs(a-b)) / 2;
    
    maiorABC = (maiorAB + c + abs(maiorAB - c)) / 2;
    
    std::cout << maiorABC << " eh o maior" << std::endl;
 
    return 0;
}