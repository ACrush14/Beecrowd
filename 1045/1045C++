#include <stdio.h>
#include <iostream>

 
int main() {
 
        double a, b, c, temp;
        
        std::cin >> a >> b >> c;
        
        if (a < b) {
            temp = a;
            a = b;
            b = temp;
        }
        
        if (a < c) {
            temp = a;
            a = c;
            c = temp;
        }
        
        if (b < c) {
            temp = b;
            b = c;
            c = temp;
        }
        
        if (a >= b + c) {
            std::cout << "NAO FORMA TRIANGULO\n";
        } else {
        if ((a*a) == (b*b) + (c*c)) {
           std::cout << "TRIANGULO RETANGULO\n";
        } else if (a*a > (b*b) + (c*c)) {
            std::cout << "TRIANGULO OBTUSANGULO\n";
        } else if ( a*a < (b*b) + (c*c)) {
             std::cout << "TRIANGULO ACUTANGULO\n";
        }
        if ((a == b) && (a == c) && (b == c)) {
             std::cout << "TRIANGULO EQUILATERO\n";
        } else if (a == b || a == c || b == c ) {
             std::cout << "TRIANGULO ISOSCELES\n";
        }
    }
 
    return 0;
}