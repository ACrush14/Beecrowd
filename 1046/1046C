#include <stdio.h>
 
int main() {
 
    int inicio, fim, total;
    
    scanf("%d %d", &inicio, &fim);
    total = 0;
    
    if (inicio == fim) {
        total = 24;
    } else if (inicio > fim) {
        fim = fim + 24;
        total = fim - inicio;
    } else {
        total = fim - inicio;
    }
    
    printf("O JOGO DUROU %d HORA(S)\n", total);
 
    return 0;
}